{
  "name": "september-werbeagentur/contao-slider-geil-bundle",
  "type": "contao-bundle",
  "description": "Slider extension for Contao Open Source CMS",
  "license": "MIT",
  "authors": [
    {
      "name": "September Werbeagentur",
      "homepage": "https://github.com/september-werbeagentur"
    }
  ],
  "require": {
    "php": "^7.1",
    "contao/core-bundle": "^4.4",
    "symfony/framework-bundle": "^3.4 || ^4.1"
  },
  "conflict": {
    "contao/core": "*",
    "contao/manager-plugin": "<2.0 || >=3.0"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.0",
    "doctrine/doctrine-cache-bundle": "^1.3",
    "phpunit/phpunit": "^5.7.26",
    "symfony/phpunit-bridge": "^3.2"
  },
  "extra": {
    "contao-manager-plugin": "SeptemberWerbeagentur\\ContaoSliderGeilBundle\\ContaoManager\\Plugin"
  },
  "autoload": {
    "psr-4": {
      "SeptemberWerbeagentur\\ContaoSliderGeilBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SeptemberWerbeagentur\\ContaoSliderGeilBundle\\": "tests/"
    }
  }
}